{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"no-control-regex": "error",
		"no-useless-escape": 0,
		"no-unused-vars": "warn",
		"no-console": 2,
		"func-names": 2,
		"no-process-exit": "off",
		"object-shorthand": 2,
		"class-methods-use-this": 2
	}
}
